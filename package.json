{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:generate": "drizzle-kit generate --config drizzle.config.migration.ts",
    "db:push": "drizzle-kit push --config drizzle.config.migration.ts",
    "db:seed": "ts-node src/seeder.ts",
    "db:studio": "drizzle-kit studio --config drizzle.config.migration.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "drizzle-orm-pg": "^0.16.3",
    "express": "^4.19.2",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.3",
    "@types/pg": "^8.11.8",
    "drizzle-kit": "^0.24.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
